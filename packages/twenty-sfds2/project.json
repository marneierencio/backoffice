{
  "name": "twenty-sfds2",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "tags": ["scope:frontend", "scope:sfds2"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "npx vite build -c vite.config.ts"
      }
    },
    "start": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "twenty-sfds2:build",
        "port": 3002
      }
    }
  }
}
